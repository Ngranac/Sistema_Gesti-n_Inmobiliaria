<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]Sistema Gestión Inmobiliaria</title>
    <link rel="shortcut icon" href="/img/favicon.ico">
    @*  <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" /> *@
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/mdb.min.css">
    <link rel="stylesheet" href="~/css/all.min.css" />
    <link href="https://cdn.jsdelivr.net/npm/tom-select@2.2.2/dist/css/tom-select.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/tom-select@2.2.2/dist/css/tom-select.bootstrap5.min.css" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.13.6/css/dataTables.bootstrap5.min.css">
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/buttons/1.6.2/css/buttons.dataTables.min.css" />
    @await RenderSectionAsync("Styles", required: false)
</head>

<body id="body">
    <header>
        <p class="tituloH">Sistema Control Inmobiliario</p>
    </header>
    <div class="menu__side" id="menu_side">
        <div class="nombre_pagina" id="btn_open">
            <span id="btn_open"><img src="~/img/log-azul.png" alt=""></span>
        </div>
        <div class="opciones__menu">

            <a asp-page="/Dashboard">
                <div class="option">
                    <i class="fa-light fa-building-memo"></i>
                    <label>Proyectos</label>
                </div>
            </a>
            <a asp-page="/Clientes">
                <div class="option">
                    <i class="fa-light fa-users" title="Clientes"></i>
                    <label>Clientes</label>
                </div>
            </a>
            <a asp-page="../Alertas/Alertas">
                <div class="option">
                    <i class="fa-regular fa-warehouse"></i>
                    <label>Bodegas</label>
                </div>
            </a>

            <a asp-page="../Reportes/Reportes">
                <div class="option text-center">
                    <i class="fa-light fa-memo-pad"></i>
                    <label>Facturación</label>
                </div>
            </a>

            <a asp-page="../SICVECA/Cuentas">
                <div class="option">
                    <i class="fa-solid fa-people"></i>
                    <label>Empleados</label>
                </div>
            </a>
            <a asp-page="../Configuracion/Configuracion">
                <div class="option">
                    <i class="fa-light fa-gear" title="Configuracion"></i>
                    <label>Configuración</label>
                </div>
            </a>

            <a asp-page="/Logout">
                <div class="option">
                    <i class="fa-solid fa-right-from-bracket" title="Cerrar Sesión"></i>
                    <label>Cerrar Sesión</label>
                </div>
            </a>
        </div>
    </div>
    @RenderBody()
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <!-- MDB ESSENTIAL -->
    <script type="text/javascript" src="~/js/mdb.umd.min.js"></script>
    <!-- MDB PLUGINS -->
    <script type="text/javascript" src="~/js/all.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/tom-select@2.2.2/dist/js/tom-select.complete.min.js"></script>
    <script defer src="https://cdn.jsdelivr.net/npm/alpinejs@3.x.x/dist/cdn.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/dayjs@1/dayjs.min.js"></script>
    <script src="https://cdn.datatables.net/1.13.6/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.13.6/js/dataTables.bootstrap5.min.js"></script>
    <script src="https://kit.fontawesome.com/4b2f294736.js" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/autonumeric@4.5.4"></script>
    <script src="https://cdn.datatables.net/buttons/1.6.2/js/dataTables.buttons.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/1.6.2/js/buttons.html5.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/1.6.2/js/buttons.print.min.js"></script>

    <!-- Include PDF make library -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/vfs_fonts.js"></script>
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
    <!--Add datatables-->
    <script>

        $(document).ready(function () {
            var table = $('#valores').DataTable({
                info: false,
                ordering: true,
                "dom": 'rtip',
                responsive: true,
                language: {
                    "decimal": "",
                    "emptyTable": "No hay información",
                    "info": "Mostrando _START_ a _END_ de _TOTAL_ Entradas",
                    "infoEmpty": "Mostrando 0 to 0 of 0 Entradas",
                    "infoFiltered": "(Filtrado de _MAX_ total entradas)",
                    "infoPostFix": "",
                    "thousands": ",",
                    "lengthMenu": "Mostrar _MENU_ Entradas",
                    "loadingRecords": "Cargando...",
                    "processing": "Procesando...",
                    "search": "Buscar:",
                    "zeroRecords": "Sin resultados encontrados",
                    "paginate": {
                        "first": "Primero",
                        "last": "Ultimo",
                        "next": "Siguiente",
                        "previous": "Anterior"
                    }
                },
            });
        });
        $(document).ready(function () {
            var table = $('#calificaciones').DataTable({
                info: false,
                ordering: true,
                dom: 'Brtip',
                buttons: [

                    {
                        extend: 'csv',
                        text: '<button class="btnTableSecond"><i class="fa-light fa-file-csv"></i></button>',
                    },
                    {
                        extend: 'excel',
                        text: '<button class="btnTableprimary"><i class="fa-light fa-file-xls"></i></button>',
                    },
                    {
                        extend: 'pdf',
                        text: '<button class="btnTableSecond"><i class="fa-light fa-file-pdf"></i></button>',
                    },
                ],
                responsive: true,
                language: {
                    "decimal": "",
                    "emptyTable": "No hay información",
                    "info": "Mostrando _START_ a _END_ de _TOTAL_ Entradas",
                    "infoEmpty": "Mostrando 0 to 0 of 0 Entradas",
                    "infoFiltered": "(Filtrado de _MAX_ total entradas)",
                    "infoPostFix": "",
                    "thousands": ",",
                    "lengthMenu": "Mostrar _MENU_ Entradas",
                    "loadingRecords": "Cargando...",
                    "processing": "Procesando...",
                    "search": "Buscar:",
                    "zeroRecords": "Sin resultados encontrados",
                    "paginate": {
                        "first": "Primero",
                        "last": "Ultimo",
                        "next": "Siguiente",
                        "previous": "Anterior"
                    }
                },
            });
        });


        $(document).ready(function () {
            var table = $('#Proyectos').DataTable({

                info: false,
                ordering: true,
                dom: 'Brtip',
                buttons: [

                    {
                        extend: 'csv',
                        text: '<button class="btnTableSecond"><i class="fa-light fa-file-csv"></i></button>',
                        exportOptions: {
                            columns: ':not(:last)'
                        }
                    },
                    {
                        extend: 'excel',
                        text: '<button class="btnTableprimary"><i class="fa-light fa-file-xls"></i></button>',
                        exportOptions: {
                            columns: ':not(:last)'
                        }
                    },
                    {
                        extend: 'pdf',
                        text: '<button class="btnTableSecond"><i class="fa-light fa-file-pdf"></i></button>',
                        exportOptions: {
                            columns: ':not(:last)'
                        }
                    },
                ],
                language: {
                    "decimal": "",
                    "emptyTable": "No hay información",
                    "info": "Mostrando _START_ a _END_ de _TOTAL_ Entradas",
                    "infoEmpty": "Mostrando 0 to 0 of 0 Entradas",
                    "infoFiltered": "(Filtrado de _MAX_ total entradas)",
                    "infoPostFix": "",
                    "thousands": ",",
                    "lengthMenu": "Mostrar _MENU_ Entradas",
                    "loadingRecords": "Cargando...",
                    "processing": "Procesando...",
                    "search": "Buscar:",
                    "zeroRecords": "Sin resultados encontrados",
                    "paginate": {
                        "first": "Primero",
                        "last": "Ultimo",
                        "next": "Siguiente",
                        "previous": "Anterior"
                    }
                },
            });
        });
        $(document).ready(function () {
            var table = $('#Archivos').DataTable({

                info: false,
                ordering: true,
                dom: 'Brtip',
                buttons: [


                    {
                        extend: 'csv',
                        text: '<button class="btnTableSecond"><i class="fa-light fa-file-csv"></i></button>',
                        exportOptions: {
                            columns: ':not(:last)'
                        }
                    },
                    {
                        extend: 'excel',
                        text: '<button class="btnTableprimary"><i class="fa-light fa-file-xls"></i></button>',
                        exportOptions: {
                            columns: ':not(:last)'
                        }
                    },
                    {
                        extend: 'pdf',
                        text: '<button class="btnTableSecond"><i class="fa-light fa-file-pdf"></i></button>',
                        exportOptions: {
                            columns: ':not(:last)'
                        }
                    },
                ],
                language: {
                    "decimal": "",
                    "emptyTable": "No hay información",
                    "info": "Mostrando _START_ a _END_ de _TOTAL_ Entradas",
                    "infoEmpty": "Mostrando 0 to 0 of 0 Entradas",
                    "infoFiltered": "(Filtrado de _MAX_ total entradas)",
                    "infoPostFix": "",
                    "thousands": ",",
                    "lengthMenu": "Mostrar _MENU_ Entradas",
                    "loadingRecords": "Cargando...",
                    "processing": "Procesando...",
                    "search": "Buscar:",
                    "zeroRecords": "Sin resultados encontrados",
                    "paginate": {
                        "first": "Primero",
                        "last": "Ultimo",
                        "next": "Siguiente",
                        "previous": "Anterior"
                    }
                },
            });
        });

    </script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>